<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>First</th>
            <th>Last</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Hero Name</th>
            <th>Power</th>
            <th>Courses</th>
        </tr>
    </thead>
    <tbody>
    {{#each student}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.first_name}}</td>
            <td>{{this.last_name}}</td>
            <td>{{this.student_email}}</td>
            <td>{{this.gender}}</td>
            <td>{{this.hero_name}}</td>
            <td>{{this.power}}</td>
            <td>{{this.Courses.instructor}}</td>
        </tr>       
  	{{/each}}
    </tbody>
</table>